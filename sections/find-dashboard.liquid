{% javascript %}

  
 
$(document).ready(function () {

  
   function fetchRegistryId() {
     let customerId = $("#customer-id").val();
  
     showLoader();
        if(customerId.length != 0)
        {
            let api_url = base_url + "api/get-registery/" + customerId;

            var settings = {
                url: api_url,
                method: "GET",
                timeout: 0,
            };

            $.ajax(settings).done(function (response) {
                
                fetchData(response.data);
            });
        }else{
           hideLoader();
        }
    }
    function fetchData(registryId) {
        
        // let customerId = $("#customer-id").val();
         let api_detail_url = base_url + "api/get-registery-detail/" + registryId;

        var settings = {
            url: api_detail_url,
            method: "GET",
            timeout: 0,
        };

        $.ajax(settings).done(function (response) {
            console.log(response.data);
            if (response.status == 200) {
                // Pre-fill form fields with data from the GET request
              $('#subHeading').empty().append(response.data.Heading);
              hideLoader();

             

            
         
              
              
            
            }
        });

     
    }
    fetchRegistryId();
  
 function showLoader() {
    
    $('#loader-container').show();
  }

  function hideLoader() {
    $('#loader-container').hide();
    
  }

 
});
{% endjavascript %}
