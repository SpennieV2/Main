<style>
  .StepListingSection .StepList {
    display: flex;
    flex-wrap: wrap;
    list-style: none;
    margin: 0;
    padding: 0 40px;
}
  .StepListingSection .StepList li {
    margin: 0 50px;
    position: relative;
    width: calc(33.333% - 100px);
}
  .StepListingSection .StepList li .StepImage {
    padding-top: 10%;
    position: relative;
}
  .StepListingSection .StepList li .StepTitle {
    align-items: center;
    background: #f0e1ce;
    border-radius: 4px;
    color: #000;
    display: inline-flex;
    font-size: 15px;
    font-weight: 500;
    height: 39px;
    justify-content: center;
    letter-spacing: .01em;
    line-height: 27px;
    margin: 30px 0 18px;
    width: 80px;
}
  .StepListingSection .StepList li h3 {
    color: #000;
    font-family: Contane;
    font-size: 22px;
    font-weight: 400;
    line-height: 27px;
    margin: 0 0 14px;
}
  .StepListingSection .StepList li p {
    color: #000;
    font-size: 14px;
    letter-spacing: .01em;
    line-height: 23px;
    margin-bottom: 0;
}
  .StepListingSection .TochBtnWrapper {
    text-align: center;
}
   .DarkBtn:hover,.BtnDark:hover{
    color:white !Important;
     background:black;
  }
  .StepListingSection,#touch{
    background:white;
    padding:10px;
  }
  .form-control.form-control-lg {
    min-height: 50px;
}
   .DarkBtn,.BtnDark {
    background: #000;
    border: 1px solid #000;
    border-radius: 0;
    color: #fff;
    font-size: 14px;
    font-weight: 500;
    height: 50px;
    letter-spacing: 1px;
    line-height: 140%;
    margin: 10px auto 0;
    max-width: 400px;
    padding: 0;
    text-align: center;
    text-decoration: none;
    text-transform: uppercase;
    width: 100%;
        display: flex;
    justify-content: center;
    align-items: center;
}
  .StepImage img{
    width:100%;
  }
  .GetFormSection {
    padding: 46px 0 60px;
}
  .GetFormSection .GetFormTop {
    margin: 0 auto 46px;
    max-width: 640px;
    text-align: center;
    width: 100%;
}
   .GetFormTop h4 {
    color: #000;
    font-family: Contane;
    font-size: 33px;
    font-weight: 400;
    line-height: 41px;
    margin: 15px 14px;
    text-align: center;
}
 .GetFormTop p {
    color: #000;
    font-size: 14px;
    letter-spacing: .01em;
    line-height: 24px;
    margin: 20px auto;
    max-width: 520px;
    padding: 0 10px;
    text-align: center;
    width: 100%;
}
  .QuestionsWizardFormWapper {
    margin: 0 auto;
    max-width: 400px;
    width: 100%;
}
  .QuestionsWizardFormWapper col-lg-6{
    padding:0px;
  }
  @media only screen and (max-width:768px){
    .BannerSection{
      height:Auto !important;
      
    }
    .StepListingSection .StepList{
      flex-direction:column;
    }
    .StepListingSection .StepList li{
      width:auto;
    }
    .BannerIframeLg img{
      width:100%
    }
  }
</style>
