  <form id="contactForm">
    <div class="GetForm">
      <div class="">
        <div class="QuestionsWizardFormWapper">
          <div class="row">
            <div class="col-lg-6 col-sm-6 col-xs-12">
              <div class="form-group">
                <label for="firstName" class="form-label">
                  First Name <span class="Mandatory">*</span>
                </label>
                <input
                  type="text"
                  id="firstName"
                  class="form-control form-control-lg"
                  required
                />
              </div>
            </div>
            <div class="col-lg-6 col-sm-6 col-xs-12">
              <div class="form-group">
                <label for="lastName" class="form-label">
                  Last Name <span class="Mandatory">*</span>
                </label>
                <input
                  type="text"
                  id="lastName"
                  class="form-control form-control-lg"
                  required
                />
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-12">
              <div class="form-group">
                <label for="email" class="form-label">
                  Email Address <span class="Mandatory">*</span>
                </label>
                <input
                  type="email"
                  id="email"
                  class="form-control form-control-lg"
                  required
                />
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-12">
              <div class="form-group">
                <label for="storeName" class="form-label">
                  Store Name <span class="Mandatory">*</span>
                </label>
                <input
                  type="text"
                  id="storeName"
                  class="form-control form-control-lg"
                  required
                />
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-12">
              <div class="form-group">
                <label for="storeLocation" class="form-label">
                  Store Location(s) <span class="Mandatory">*</span>
                </label>
                <input
                  type="text"
                  id="storeLocation"
                  class="form-control form-control-lg"
                  required
                />
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-12">
              <div class="form-group">
                <label for="message" class="form-label">
                  Message (optional)
                </label>
                <input
                  type="text"
                  id="message"
                  class="form-control form-control-lg"
                />
              </div>
            </div>
          </div>

          <button type="submit" class="btn btn-flat BtnDark">Submit</button>
        </div>
      </div>
    </div>
  </form>
</section>
<script>
  document.getElementById('contactForm').addEventListener('submit', async function (event) {
  event.preventDefault();

  // Your EmailJS service ID
  const serviceId = 'service_dmcaspc';
  // Your EmailJS template ID
  const templateId = 'template_ah20ky8';
  // Your EmailJS user ID
  const userId = 'T5kWlPYiOPW3RKQPo';

  // Get form values
  const firstName = document.getElementById('firstName').value;
  const lastName = document.getElementById('lastName').value;
  const email = document.getElementById('email').value;
  const storeName = document.getElementById('storeName').value;
  const storeLocation = document.getElementById('storeLocation').value;
  const message = document.getElementById('message').value;

  // Prepare the email parameters
  const templateParams = {
    firstName,
    lastName,
    email,
    storeName,
    storeLocation,
    message,
  };

  try {
    // Send email using EmailJS
    await emailjs.send(serviceId, templateId, templateParams, userId);

    // Clear form fields after successful submission
    document.getElementById('contactForm').reset();

    // Show success notification
    console.log('Email sent successfully!');
  } catch (error) {
    // Handle errors
    console.error('Error sending email:', error);
    console.log('Error sending email. Please try again later.');
  }
});

</script>
