{% javascript %}

 $(document).ready(function () {
   function showLoader() {
    $('#loader-container').show();
  }
 function hideLoader() {
    $('#loader-container').hide();
  }
  

   
    $(document).on('change','.active_registry',function(){
      let status='';   
     
     if($(".active_registry").prop("checked") == true)
      {
             status=1;
             $(".active_registry_text").empty().append('ACTIVE');
      }
      else{
             status=0;
             $(".active_registry_text").empty().append('DEACTIVE');
          }
      

      let customerId = $("#customer-id").val(); 
      let api_url = base_url + "api/get-registery/" + customerId;
      var settings = {
          url: api_url,
          method: "GET",
          timeout: 0,
      };
    
      $.ajax(settings).done(function (response) {
        let registry_id = response.data;
        var form = new FormData();
        form.append("registry_id", registry_id);
        form.append("status", status);
        let api_url = base_url + "api/update-registry-status";
  
        var settings = {
          "url": api_url,
          "method": "POST",
          "timeout": 0,
          "processData": false,
          "mimeType": "multipart/form-data",
          "contentType": false,
          "data": form
        };
        $.ajax(settings).done(function (response) {
          console.log(response);
          const responseData = JSON.parse(response);
          toastr.success(responseData.msg);

          
        });
        
      });
      
      
    });
    $('#file').change(function (event) {
        var input = event.target;
        var reader = new FileReader();

        reader.onload = function () {
            var dataURL = reader.result;
            $('#file_upload').attr('src', dataURL); // Update the src attribute with the new image data
            $('.banner').attr('src', dataURL);
        };

        reader.readAsDataURL(input.files[0]);
    });
    

    function fetchRegistryId() {
     let customerId = $("#customer-id").val();
     showLoader();
     let api_url = base_url + "api/get-registery/" + customerId;

            var settings = {
                url: api_url,
                method: "GET",
                timeout: 0,
            };

            $.ajax(settings).done(function (response) {
                // console.log("Registry ID:", response.data);
                // Now that you have the registry ID, make another API request for detailed information
                fetchData(response.data);
            });
    }
    // Function to fetch data from the API using GET request
    function fetchData(registryId) {
        
        // let customerId = $("#customer-id").val();
         let api_detail_url = base_url + "api/get-registery-detail/" + registryId;

        var settings = {
            url: api_detail_url,
            method: "GET",
            timeout: 0,
        };

        $.ajax(settings).done(function (response) {
            console.log(response.data);
            if (response.status == 200) {

               var state_settings = {
                "url":  base_url + "api/get-state",
                "method": "GET",
                "timeout": 0,
              };
        
              $.ajax(state_settings).done(function (state_response) {

                  $('#subHeading').val(response.data.subHeading);
                  $('#subHeading-2').text(response.data.subHeading);
                  $('#WebsiteUrl-2').attr('href', response.data.WebsiteUrl);
                  $('#Heading').val(response.data.Heading);
                  $('#HeadingNEW').text(response.data.Heading);
                  $('#shippment_address').val(response.data.shippingAddress);
                  $('#unit').val(response.data.unit);
                  $('#City').val(response.data.city);
                  $('#zip').val(response.data.zipCode);
       
                  if(response.data.isLive == 0)
                  {
                    $(".active_registry").prop('checked', false);
                    $(".active_registry_text").empty().append('DEACTIVE');
                  }
                  else{
                      $(".active_registry").prop('checked', true);
                      $(".active_registry_text").empty().append('ACTIVE');
     
                  }

                  if(response.data.isDateNotDecided == 0)
                  {
                    $('select[name="event-month"]').val(response.data.Month);
                    $('.event-day').val(response.data.Day);
                    $('.event-year').val(response.data.Year);
                    
                  }
                  else{
                      $("#check_id").prop('checked', true);
                      $('select[name="event-month"]').prop('disabled', true);
                      $('.event-day').prop('disabled', true);
                      $('.event-year').prop('disabled', true);

                  }

                   const imageBanner = document.getElementById('file_upload');
                  if (response && response.data && response.data.img) {   
                    imageBanner.src = response.data.img;
                  } else {  
                    imageBanner.src = 'data:image/png;base64,';
                  }

                  const imageBanner2 = document.getElementById('image_banner-2');
                  if (response && response.data && response.data.img) {   
                    imageBanner2.src = response.data.img;
                  } else {  
                    imageBanner2.src = 'data:image/png;base64,';
                  }
               
              
                  $('#Wedding_Website_Url').val(response.data.WebsiteUrl);
                  $('.event-day').val(response.data.Day);
                  $('.event-year').val(response.data.Year);

                  
                  $(".select-state").empty();
                  let data = state_response.data;

                  data.forEach((item) => {
                      // Check if the current option matches response.data.state
                      let selectedAttribute = (response.data.state === item.name) ? 'selected' : '';
                      
                      // Append option to the select dropdown
                      $(".select-state").append(`<option value="${item.name}" ${selectedAttribute}>${item.name}</option>`);
                  });
              });
               
             
            
            }
        });

  
        var settings = {
          "url": base_url + "api/get-registry-products/" +registryId,
          "method": "GET",
          "timeout": 0,
        };
      
        $.ajax(settings).done(function (response) {

                hideLoader();
               // const responseData = JSON.parse(response);
               // const responseData = JSON.parse(response);
                console.log(response['data']);
                let product=response['data'];
                $(".product_row").empty();
                if(response.status ==200){
                    product.forEach((product) => {
                  
                  console.log(product.variant.variant.price);
                  let qty        =product.variant.variant_qty;
                  let variant_id =product.variant_id;
                  let product_id =product.product_id;
                  let product_name ="";
                  
                  
                  $(".product_row").append(`<div class="col-md-4">
                                          <div class="product-card">
                                    		<div class="product-tumb">
                                    			<img src="${product.img_src}" alt="" />
                                    		</div>
                                    		<div class="product-details">
                                    			<div class="product-bottom-details">
                                                  	<h4 class="product-title"> <a href="">${product.title}</a></h4>
                                    				<div class="product-price">$ ${product.variant.variant.price} </div>
                                    			</div>
                                              <div class="btn-checkout">
                                                  <button>Add To Cart<br> Requested Quantity: ${qty} | Still needs :  ${qty}</button>

                                              </div>
                                              
                                    		</div>
                                    	</div>
                                    </div>`);
                });
                }
          });

        var settings = {
          "url":  base_url + "api/get-state",
          "method": "GET",
          "timeout": 0,
        };
  
        $.ajax(settings).done(function (response) {
          let data=response.data;
          data.forEach((data) => {

             $(".select-state").append(`<option value="${data.name}">${data.name}</option>`);
            
          });
        });
         
    }

    fetchRegistryId();

    // Add an event listener to the submit button
    $("#Heading").on("keyup", function () {
      let data=$(this).val();
      $(".registry-heading").empty().append(data);
    });
    $("#subHeading").on("keyup", function () {
      
      let data=$(this).val();
       $("#subHeading-2").empty().append(data);
     

    });
    $("#submitFormBtn").on("click", function () {
        if($("#shippment_address").val().length == 0)
        {
          toastr.error("Shipping Address is required");
          return false;
    
        }
        if($("#select-state").val().length == 0)
        {
          toastr.error("State is required");
          return false;
    
        }
        if($("#City").val().length == 0)
        {
          toastr.error("City is required");
          return false;
    
        }
        if($("#zip").val().length == 0)
        {
          toastr.error("zip code is required");
          return false;
    
        }
        var formData = new FormData();
        // Add form fields to the FormData object
        formData.append("customer_id", $("#customer-id").val());
        formData.append("shippingAddress", $("#shippment_address").val());
        formData.append("unit", $("#unit").val());
        formData.append("city", $("#City").val());
        formData.append("state", $("#select-state").val());
        formData.append("zipCode", $("#zip").val());
        formData.append("WebsiteUrl", $("#Wedding_Website_Url").val());
        formData.append("Heading", $("#Heading").val());
        formData.append("sub_Heading", $("#subHeading").val());
        formData.append("event_month", $("#gMonth2").val()); 
        formData.append("event_day", $(".event-day").val());
        formData.append("event_year", $("#event_year").val());
        var fileInput = $("#file")[0].files[0];
        formData.append("file", fileInput);
        if($("#check_id").prop("checked") == true)
        {
          formData.append("event","on" );
        }
  
        else{
          formData.append("event", "off");
        }

       
      //form.append("file", fileInput.files[0], "");
      
      var settings = {
        "url": base_url + "api/update-registery",
        "method": "POST",
        "timeout": 0,
        "processData": false,
        "mimeType": "multipart/form-data",
        "contentType": false,
