<style>
  body{
    font-family: -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Oxygen, Ubuntu, Cantarell, Fira Sans, Droid Sans, Helvetica Neue, sans-serif !important;
  }
  .ContactSection .ContactDetails h2 {
    color: #000;
    font-family: Contane;
    font-size: 33px;
    font-weight: 400;
    line-height: 41px;
    margin: 0 0 20px;
    text-align: center;
}
  .ContactSection {
    align-items: center;
    background-color: #f8f8f8;
    display: flex;
    height: calc(100vh - 304px);
    justify-content: center;
    min-height: 500px;
    text-align: center;
  }
  .ContactSection .ContactDetails {
    max-width: 640px;
    width: 100%;
   }
  .ContactSection .ContactDetails h2 {
    color: #000;
    font-family: Contane;
    font-size: 33px;
    font-weight: 400;
    line-height: 41px;
    margin: 0 0 20px;
    text-align: center;
  }
  .ContactSection .ContactDetails p {
    color: #000;
    font-size: 14px;
    letter-spacing: .01em;
    line-height: 24px;
    margin: 0 auto;
    max-width: 444px;
    text-align: center;
  }
  .ContactSection .ContactDetails p:not(:last-child) {
    margin-bottom: 15px;
   }
  .ContactSection .ContactDetails .ContactListing {
    display: flex;
    flex-wrap: wrap;
    list-style: none;
    margin: 20px 0 0;
    padding: 0;
   }
  .ContactSection .ContactDetails .ContactListing li {
    color: #000;
    font-size: 15px;
    letter-spacing: .01em;
    line-height: 33px;
    text-align: center;
    width: 33.333%;
  }
  .ContactSection .ContactDetails .ContactListing li .TextLabels {
    font-family: Neue Haas Unica W1G;
    font-size: 15px;
    font-style: normal;
    font-weight: 400;
    line-height: 33px;
   }
  .ContactSection .ContactDetails .ContactListing li .TextDetails {
    font-weight: 600;
    letter-spacing: .07em;
    text-decoration: underline;
    text-transform: uppercase;
   }
  .ContactSection .ContactDetails .ContactListing li .TextDetails a {
    color: #000;
    font-size: 14px;
   }
</style>
<section class="ContactSection">
    <div class="ContactDetails">
        <h2>{{ section.settings.heading}}</h2>
        <p>{{ section.settings.line1}}</p>
        <p>{{ section.settings.line2}}</p>
        <ul class="ContactListing">
            <li>
                <div class="TextLabels">{{ section.settings.TextLabels1}}</div>
                <div class="TextDetails"><a href="mailto:{{ section.settings.TextDetails1}}" target="_blank">{{ section.settings.TextDetails1}}</a></div>
            </li>
            <li>
                <div class="TextLabels">{{ section.settings.TextLabels2}}</div>
                <div class="TextDetails"><a href="https://www.instagram.com/{{ section.settings.TextDetails2}}/" target="_blank"
                        rel="noopener noreferrer">@{{ section.settings.TextDetails2}}</a></div>
            </li>
            <li>
                <div class="TextLabels">{{ section.settings.TextLabels3}}</div>
                <div class="TextDetails"><a href="sms:{{ section.settings.TextDetails3}}">{{ section.settings.TextDetails3}}</a></div>
            </li>
        </ul>
    </div>
</section>
{% schema %}
  {
    "name": "contact",
    "settings": [
        {
          "type": "text",
          "id": "heading",
          "label": "Heading"
        },
        {
          "type": "text",
          "id": "line1",
          "label": "line 1"
        },
        {
          "type": "text",
          "id": "line2",
          "label": "line 2"
        },
        {
          "type": "text",
          "id": "TextLabels1",
          "label": "TextLabels 1"
        },
        {
          "type": "text",
          "id": "TextDetails1",
          "label": "TextDetails 1"
        },
        {
          "type": "text",
          "id": "TextLabels2",
          "label": "TextLabels 2"
        },
        {
          "type": "text",
          "id": "TextDetails2",
          "label": "TextDetails 2"
        },
        {
          "type": "text",
          "id": "TextLabels3",
          "label": "TextLabels 3"
        },
        {
          "type": "text",
          "id": "TextDetails3",
          "label": "TextDetails 3"
        }
    ]
  }
{% endschema %}