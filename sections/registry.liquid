{% javascript %}

  
 
$(document).ready(function () {
  
  
 function showLoader() {
    
    $('#loader-container').show();
  }

  function hideLoader() {
    $('#loader-container').hide();
    
  }

  async function fetchData(registryId) {
    showLoader();
    let api_url = base_url + "api/get-registery-detail/" + registryId;
    var settings = {
      url: api_url,
      method: "GET",
      timeout: 0,
    };

    $.ajax(settings).done(function (response) {
      

      console.log(response.data);

      if (response.status == 200) {
       
        $('#subHeading').text(response.data.subHeading);
        $('#Heading').text(response.data.Heading);
        if (response.data.WebsiteUrl) {
                $('#WebsiteUrl').attr('href', response.data.WebsiteUrl).show();
            } else {
                $('#WebsiteUrl').hide();
            }
        $('#Day').text(response.data.Day);
        $('#Month').text(response.data.Month);
        $('#Year').text(response.data.Year);
        $('.event-day').val(response.data.Day);
        $('.event-year').val(response.data.Year);
        
            const imageBanner = document.getElementById('image_banner-3');
            if (response && response.data && response.data.img) {  


               
              imageBanner.src = response.data.img;
                   
              
            } else {  
              imageBanner.src = 'data:image/png;base64,';
            }
      }
    }).fail(function () {
    
      // Handle failure or show an error message
    });
    var count = 1;
var isLoading = false;

function fetchProducts() {
    isLoading = true;

    var settings = {
        "url": base_url + "api/get-registry-products/" + registryId + '/' + count,
        "method": "GET",
        "timeout": 0,
    };

    $.ajax(settings).done(function(response) {
        hideLoader();
        console.log(response['data']);
        let products = response['data'];

        products.forEach((product, index) => {
            if (product.variant.variant) {
                let qty = product.variant.variant_qty;
                let needqty = product.variant.still_need_qty;
                if (needqty == null) {
                    needqty = qty;
                }
                let variant_id = product.variant.variant.id;
                let product_id = product.product_id;

                console.log(product.variant.variant.title);

                let img_src = product.img_src; // Assuming img_src is an array
                let defaultImg = img_src[0];
                let hoverImg = img_src.length > 1 ? img_src[1] : defaultImg;

                // Use a unique id for each image element
                let imgId = `product-img-${index}`;

                $(".product_row").append(`
                    <div class="col-md-3">
                        <div class="product-card">
                            <div class="product-tumb">
                                <img id="${imgId}" src="${defaultImg}" alt="" data-hover="${hoverImg}" />
                            </div>
                            <div class="product-details">
                                <div class="product-bottom-details">
                                    <h4 class="product-title">
                                        <a href="/products/${product.product_name.toLowerCase()
                                            .trim()
                                            .replace(/\s+/g, '-')
                                            .replace(/[^a-z0-9-]+/g, '')
                                            .replace(/-+/g, '-')
                                            .replace(/^-+|-+$/g, '')}">${product.title}</a>
                                    </h4>
                                    <div class="product-price">$ ${product.variant.variant.price}</div>
                                </div>
                                <div class="btn-checkout">
                                    <div class="quick-add no-js-hidden">
                                        <product-form data-section-id="template--17367260201210__product-grid">
                                            <form method="post" action="" id="quick-add-template--17367260201210__product-grid${variant_id}" accept-charset="UTF-8" class="form" enctype="multipart/form-data" novalidate="novalidate" data-type="add-to-cart-form">
                                                <input type="hidden" name="form_type" value="product">
                                                <input type="hidden" name="utf8" value="✓">
                                                <input type="hidden" name="id" value="${variant_id}" class="product-variant-id">
                                                <button id="quick-add-template--17367260201210__product-grid${product_id}-submit" type="button" name="add" data-need="${needqty}" data-id="${variant_id}" class="add-to-cart" aria-haspopup="dialog" aria-labelledby="quick-add-template--17367260201210__product-grid${product_id}-submit title-template--17367260201210__product-grid-${product_id}" aria-live="polite" data-sold-out-message="true">
                                                    <span>Add to cart<br> Requested Quantity: ${qty} | Still needs : ${needqty}</span>
                                                </button>
                                                <input type="hidden" name="product-id" value="${product_id}">
                                                <input type="hidden" name="section-id" value="template--17367260201210__product-grid">
                                            </form>
                                        </product-form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                `);

                // Add hover event listeners to the image
                let productImg = document.getElementById(imgId);
                if (img_src.length > 1) {
                    productImg.addEventListener('mouseover', function() {
                        productImg.src = hoverImg;
                    });
                    productImg.addEventListener('mouseout', function() {
                        productImg.src = defaultImg;
                    });
                }
            }
        });
        isLoading = false; // Reset loading state
    });
}


// Initial fetch
fetchProducts();

// Scroll event listener
// $(window).on('scroll', function() {
//     if ($(window).scrollTop() + $(window).height() >= $(document).height() - 100 && !isLoading) {
//         count++;
//         fetchProducts();
//     }
// });
  }

  async function getRegistryIdFromUrl() {
    let currentUrl = window.location.href;
    let segments = currentUrl.split('/');
    let registryId = segments[segments.length - 1];
    if(registryId == 'my-registry')
    {



         let customerId = $("#customer-id").val();
         let api_url = base_url + "api/get-registery/" + customerId;

                var settings = {
                    url: api_url,
                    method: "GET",
                    timeout: 0,
                };

                $.ajax(settings).done(function (response) {
                     

                  
                    location.href = currentUrl+"/"+response.data;
                });
            
      
    }
    else{
      return registryId;
    }
   
    
  }

  let registryId = getRegistryIdFromUrl();

  

  
  async function initialize() {
      let registryId = await getRegistryIdFromUrl();
    
      if (typeof registryId !== 'undefined') {
          await fetchData(registryId);
      }
  }
  
  initialize();
  
  
  $(document).on('click', '.add-to-cart', function(event) {

    
      event.preventDefault();
      let data_id=$(this).attr('data-id');
      let data_need=$(this).attr('data-need');
    
     if(data_need == 0)
     {
        toastr.success("Requested quantity completed");
        return false;
       
     }
      var currentUrl = window.location.href;

      var id = currentUrl.split('/my-registry/')[1];

      var name = $(".registry-heading").text();


    
      localStorage.setItem("cart_registry_id", id);
      
      var formData = $('#quick-add-template--17367260201210__product-grid'+data_id).serialize();

     formData += '&properties[RegistryId]=' + encodeURIComponent(id);
     formData += '&properties[Registry]=' + encodeURIComponent(name);
    console.log(formData);
      // Make an AJAX request
      $.ajax({
        url: '/cart/add.js',
        method: 'POST',
        data: formData,
        dataType: 'json',
        success: function(data) {
          console.log('Product added to cart:', data);
          var firstSpanText = $('.cart-count-bubble span:first').text();

          let length = firstSpanText;
          
         
          if(length == '')
          {
            
            firstSpanText=0;
          }
          firstSpanText++;

          
          $(".cart-notification-product").empty().append(`<div class="cart-notification-product__image global-media-settings">
          <img src="${data.image}" alt="${data.title}" width="70" height="70" loading="lazy">
        </div><div><h3 class="cart-notification-product__name h4">${data.title}</h3>
        <dl></dl></div>`);
           $("#cart-icon-bubble").empty().append(`<svg class="icon icon-cart" aria-hidden="true" focusable="false" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 40 40" fill="none">
                                                    <path fill="currentColor" fill-rule="evenodd" d="M20.5 6.5a4.75 4.75 0 00-4.75 4.75v.56h-3.16l-.77 11.6a5 5 0 004.99 5.34h7.38a5 5 0 004.99-5.33l-.77-11.6h-3.16v-.57A4.75 4.75 0 0020.5 6.5zm3.75 5.31v-.56a3.75 3.75 0 10-7.5 0v.56h7.5zm-7.5 1h7.5v.56a3.75 3.75 0 11-7.5 0v-.56zm-1 0v.56a4.75 4.75 0 109.5 0v-.56h2.22l.71 10.67a4 4 0 01-3.99 4.27h-7.38a4 4 0 01-4-4.27l.72-10.67h2.22z"></path>
                                                  </svg>
                                                  <span class="visually-hidden">Cart</span><div class="cart-count-bubble"><span aria-hidden="true">${firstSpanText}</span><span class="visually-hidden">${firstSpanText} items</span></div>`);

          $("#cart-notification-button").empty().append(`View cart (${firstSpanText})`);

          $(".cart-notification").addClass('active');
        },
        error: function(error) {
        

          
          
           toastr.error(error['responseJSON']['description']);

          
        }
      });
    });
});
